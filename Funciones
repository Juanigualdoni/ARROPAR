Fucion 2:

CREATE FUNCTION 'Articulos XL`(Talle VAR) 
RETURNS varchar(10) 
Reads sql data
BEGIN
DECLARE a VARCHAR(10);

IF talle = 'XL' then
RETURN 'OK';
ELSE
RETURN 'NO ES XL';
END IF;

END
