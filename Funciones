Funci√≥n 1:

CREATE FUNCTION `Raiz cuadrada` (Id_usuarios INT) 
RETURNS float
reads sql data
BEGIN
	DECLARE total FLOAT;
    SET Total = (SELECT SQRT(Telefono) FROM arropar.usuarios WHERE id = Id_usuarios);

RETURN TOTAL;
END


